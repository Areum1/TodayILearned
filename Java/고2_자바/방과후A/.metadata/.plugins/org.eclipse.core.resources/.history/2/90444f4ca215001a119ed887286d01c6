
public class Radio implements Volume {
	// 오버라이딩 안 해줘서 에러생김
	
	private int volLevel;
	
	public Radio() {
		volLevel = 0;
	}
	
	@Override
	public void volumeUp(int level) {
		volLevel += level;
	} // volumeup

	@Override
	public void volumeDown(int level) {
		volLevel -= level;
	} // volumedown

	
}
