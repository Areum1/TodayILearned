import java.util.*;
class ran_arr{
public static void main (String ar[]) {
Random r = new Random();
int i = 0,j = 0;
int n;
int a[][] = new int [4][3];
for (i = 0; i<4; i++)
{
	for (j = 0; j<3; j++)
	{
		n = r.nextInt(100);
		a[i][j] = n;
		System.out.print(a[i][j]+" ");
	}
	System.out.println("");
}
}
}