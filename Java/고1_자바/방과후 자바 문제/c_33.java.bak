import java.util.*;
class c_33 {
public static void main(String ar[]){
	int m = 1;
	Scanner sc = new Scanner(System.in);
	int i = sc.nextInt();
	int j = sc.nextInt();
	int a[][] = new int[i][j];
	for (int c = 0; c<i; c++) {
		for (int b = 0; b<j; b++) {
		a[c][b] = m;
		m++;
		}
	}
		for (int c = 0; c<3; c++) {
		for (int b = 0; b<4; b++)
			System.out.print(a[c][b]+"\t");
	System.out.println();
	}
}
}